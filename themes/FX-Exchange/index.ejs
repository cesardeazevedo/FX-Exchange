      </div>

      <!-- bitex-view-content -->
      <div class="bitex-view-content">
          <!-- panel-group  -->
          <div class="panel-group" id="accordion" role="tablist" aria-multiselectable="true">

              <% for(var post in public.en_US.posts._data){ %>
              <!-- panel panel-default -->
              <div class="panel panel-default">
                  <div class="panel-heading" role="tab" id="heading-<%- post %>">
                      <h3 class="panel-title">
                          <a data-toggle="collapse" data-parent="#accordion" href="#<%- post %>" aria-expanded="true" aria-controls="<%- post %>">
                              <%= public.en_US.posts._data[post].title %>
                          </a>
                      </h3>
                  </div>
                  <!-- panel-collapse collapse-->
                  <div id="<%- post %>" class="panel-collapse collapse" role="tabpanel" aria-labelledby="heading-<%- post %>">
                      <div class="panel-body">
                          <% if(lang == "en_US") { %>
                          <% var p = public.en_US.posts._data[post]; %>
                          <% p.yield = partial("en_US/posts/"+post); %>
                          <%- partial("/en_US/posts/_post", p) %>
                          <% } %>
                      </div>
                  </div>
                  <!-- panel-collapse collapse-->
              </div>
              <!-- panel panel-default -->
              <% } %>
          </div>
          <!-- panel-group  -->
          <h3>
              <a href="" class="bitex-support-link" target="_blank">
                  <i class="glyphicon glyphicon-envelope"></i> <%= t.views.faq.get_in_touch %>
              </a>
          </h3>
      </div>
      <!-- bitex-view-content -->
  </div>
