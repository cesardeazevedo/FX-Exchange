                <img src="<%= baseurl %>/assets/exchange_logos/<%= exchange_logo %>_landing_page_logo.png" style="">
                <p class="lead bitex-view-title bitex-view-title">
                <!-- translate variable -->
                <% var t%>
                    <%  
                    if(current.path.length == 0) {
                        t = (lang == "en_US") ?
                            public.en_US._data : (lang == "es") ?
                            public.es._data    : (lang == "pt_BR") ?
                            public.pt_BR._data : public.en_US._data
                    } else {
                        t = (current.path[0] == "en_US") ?
                            public.en_US._data : (current.path[0] == "es") ?
                            public.es._data    : (current.path[0] == "pt_BR") ?
                            public.pt_BR._data : public.en_US._data
                    }
                    %>
                <% if (exchange  == 'chilebit'){  %>
                        <%= t.exchanges.chilebit.lead  %>
                <% } %>
                <% if (exchange  == 'surbitcoin'){  %>
                        <%= t.exchanges.surbitcoin.lead  %>
                <% } %>
                <% if (exchange == 'demo') { %>
                        <%= t.exchanges.demo.lead  %>
                <% } %>
                </p>
                <span class="text-center">
                    <a href="" class="btn btn-success btn-lg" data-switch-view="signup"><%= t.views.start.signup %></a>
                </span>
                <script>
                    setInterval(function(){
                    console.log("<%= current.path[0] %>");
                    },2000);
                </script>
                <!-- row-fluid -->
            </header>
