    <% if (load_application) { %>
    <script data-cfasync="false" type="text/javascript">
      var _app;  // global application object
      (function(){
          var broker_id = <% if (blinktrade_broker_id) { %><%- blinktrade_broker_id %><% } else { %><%- blinktrade_broker_id %><% } %>;
          var country_code = "<% if (default_country_code){ %><%- default_country_code %><% } else { %><%- default_country_code %><% } %>";
          var blinktrade_api = "<% if (blinktrade_backend_api) { %><%- blinktrade_backend_api %><% } else { %><%- blinktrade_backend_api %><% } %>";

        _app = new BlinkTradeApp(broker_id, country_code );
        _app.run(blinktrade_api);
      })();
    </script>
    <% } %>

